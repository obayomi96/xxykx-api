{
  "name": "comments-api",
  "version": "1.0.0",
  "description": "Simple comments API with authentication",
  "main": "index.js",
  "scripts": {
    "build": "npm run clean && npm run migrate && npm run seed && babel ./src --out-dir ./build",
    "start": "node build/index.js",
    "dev": "export DEBUG=dev && nodemon --exec babel-node src/app.js",
    "clean": "rm -rf build && mkdir build",
    "lint": "eslint .",
    "seed": "npm run seed:all",
    "seed:all": "node_modules/.bin/sequelize db:seed:all",
    "seed:undo": "node_modules/.bin/sequelize db:seed:undo:all",
    "generate:model": "node_modules/.bin/sequelize model:generate",
    "generate:migration": "node_modules/.bin/sequelize migration:generate",
    "generate:seed": "node_modules/.bin/sequelize seed:generate",
    "migrate": "node_modules/.bin/sequelize db:migrate",
    "migrate:undo": "node_modules/.bin/sequelize db:migrate:undo:all"
  },
  "keywords": [],
  "author": "Martins Obayomi",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "debug": "^4.1.1",
    "dotenv": "^8.2.0",
    "errorhandler": "^1.5.1",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "faker": "^4.1.0",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "pg": "^8.2.1",
    "pg-hstore": "^2.3.3",
    "sequelize": "^5.21.13",
    "sequelize-cli": "^5.5.1",
    "uuid": "^8.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.1",
    "@babel/core": "^7.10.2",
    "@babel/node": "^7.10.1",
    "@babel/preset-env": "^7.10.2",
    "@babel/runtime": "^7.10.2",
    "eslint": "^7.2.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-prettier": "^3.1.4",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5"
  }
}
